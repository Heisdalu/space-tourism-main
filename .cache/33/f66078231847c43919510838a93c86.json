@use \"functions\" as f;\r\n@use \"variables\" as var;\r\n\r\n@mixin google-font($font){\r\n    $font: unquote($font);\r\n    @import url(http://fonts.googleapis.com/css?family=#{$font});\r\n}\r\n@mixin bg-default{\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    background-position: center;\r\n}\r\n@mixin double-grid{\r\n    display: grid;\r\n    grid-template-rows: auto 1fr;\r\n    @media only screen and (min-width: 800px){\r\n        grid-template-columns: 1fr 1fr;\r\n        grid-template-rows: none;\r\n    }\r\n}\r\n@mixin bd-radius($radius){\r\n    border-radius: $radius;\r\n}\r\n@mixin circle{\r\n    width: f.em(70px);\r\n    height: f.em(70px);\r\n    background-color: white;\r\n    @include bd-radius(50%);\r\n    color: var.$base-color;\r\n    @include flex-align;\r\n}\r\n@mixin flex-align($flex-dir: row, $align-itm: center, $jst-cnt: center){\r\n    display: flex;\r\n    align-items: $align-itm;\r\n    justify-content: $jst-cnt;\r\n    flex-direction: $flex-dir;\r\n}\r\n@mixin hover-state{\r\n    &:hover{\r\n        border-bottom: 2px solid white;\r\n    }\r\n}","scss\\partials\\_default.scss":"@use \"variables\" as *;\r\n@use \"mixins\" as *;\r\n@use \"functions\" as *;\r\n@include google-font(Bellefair);\r\n@include google-font(Barlow);\r\n@include google-font(Barlow+Condensed);\r\n*,\r\n*::before,\r\n*::after{\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n}\r\nbody{\r\n    font-size: $ft-size;\r\n    font-family: Barlow, sans-serif;\r\n    line-height: 1.5;\r\n    word-spacing: .15;\r\n    letter-spacing: .18;\r\n    color: white;\r\n    @include bg-default;\r\n}\r\nhtml,\r\nbody{\r\n    overflow-x: hidden;\r\n    min-height: 100vh;\r\n}\r\nh1,h2,h3,h4,h5,h6{\r\n    font-family: 'Bellefair', serif;\r\n    font-weight: 400;;\r\n    line-height: 1.5;\r\n}\r\nh1{\r\n    font-size: em(80px);\r\n    text-transform: uppercase;\r\n    line-height: 1.2;\r\n}\r\nh2{\r\n    font-size: em(50px);\r\n}\r\nh3{\r\n    font-size: em(27px);\r\n}\r\nh4{\r\n    font-size: em(25px);\r\n}\r\nh5{\r\n    font-family: 'Barlow Condensed';\r\n    font-size: em(20px);\r\n}\r\nimg,\r\nsource{\r\n    max-width: 100%;\r\n}\r\nul{\r\n    list-style: none;\r\n    font-size: 1em;\r\n}\r\nli{\r\n    font-size: 1em;\r\n}\r\na{\r\n    text-decoration: none;\r\n    color: inherit;\r\n    font-size: inherit;\r\n    //=== active state ===\r\n    @include hover-state;\r\n    padding-block: 1em;\r\n}\r\nbutton{\r\n    border: none;\r\n    background-color: transparent;\r\n    font-size: 1em;\r\n    color: inherit;\r\n    font-family: inherit;\r\n}\r\n.wrapper{\r\n    max-width: em(900px);\r\n    margin-inline: auto;\r\n    padding: 1em;\r\n}\r\n// === LAYOUT STYLES ===\r\n.hd{\r\n    display: grid;\r\n    grid-template-rows: auto 1fr;\r\n    min-height: 100vh;\r\n}\r\n.hd__nav{\r\n    position: relative;\r\n    @include flex-align($flex-dir: row, $jst-cnt: space-between, $align-itm: center);\r\n    padding-block: .5em;\r\n    @media only screen and (min-width: 800px){\r\n        &::before{\r\n            content: \"\";\r\n            position: absolute;\r\n            width: 70%;\r\n            border: 1px solid hsla(0, 0%, 100%, 0.2);\r\n            top: 50%;\r\n            left: 10%;\r\n        }\r\n    }\r\n}\r\n.hd__nav__ul{\r\n    margin-left: auto;\r\n    @include flex-align($flex-dir: row);\r\n    background-color: rgba(122, 121, 121, 0.295);\r\n    padding: 1em;\r\n    backdrop-filter: blur(10px);\r\n    gap: .8em;\r\n    \r\n    > * ~ *{\r\n        margin-left: .8em;\r\n    }\r\n    @media only screen and (max-width: 800px){\r\n        position: absolute;\r\n        top: 0%;\r\n        right: -100%;\r\n        min-height: 100vh;\r\n        width: 70%;\r\n        transition: .5s ease-in-out;\r\n        @include flex-align($flex-dir: column, $align-itm: flex-start, $jst-cnt: flex-start);\r\n        padding-top: 4em;\r\n        color: $base-color;\r\n        font-size: 1.2em;\r\n        font-weight: 600;\r\n\r\n        \r\n        li{\r\n            margin-left: 0;\r\n            margin-block: 0;\r\n            \r\n        }\r\n    }\r\n    *{\r\n        flex-basis: auto;\r\n    }\r\n}\r\n.show-link{\r\n    right: 0%;\r\n}\r\n.logo-container{\r\n    margin-left: 1em;\r\n    padding-inline: 1em;\r\n    z-index: 10;\r\n    > *{\r\n        flex-basis: auto;\r\n        flex-grow: 0;\r\n    }\r\n    @media only screen and (max-width: 800px){\r\n        &{\r\n            @include flex-align($jst-cnt: flex-start, $flex-dir: row);\r\n            width: 100%;\r\n        }\r\n    }\r\n    .hamburger {\r\n        cursor: pointer;\r\n        margin-left: auto;\r\n\r\n    }\r\n}\r\n.main{\r\n    @include double-grid;\r\n    place-items: center;\r\n    height: 100%;\r\n    gap: 4em;\r\n}\r\n.main__article{\r\n    padding-inline: 1em;\r\n    @media only screen and (max-width: 800px){\r\n        text-align: center;\r\n    }\r\n    > * ~ *{\r\n        margin-top: .1em;\r\n    }\r\n}\r\n.hamburger{\r\n    display: none;\r\n    @media only screen and (max-width: 800px){\r\n        &{\r\n            display: block;\r\n        }\r\n    }\r\n}\r\n.soft-hd{\r\n    max-width: 900px;\r\n    margin-inline: auto;\r\n    text-align: left;\r\n    @media only screen and (max-width: 800px){\r\n        &{\r\n            text-align: center;\r\n            \r\n        }\r\n    }\r\n    span{\r\n        color: rgb(133, 131, 131);\r\n        display: inline-block;\r\n        padding-right: .2em;\r\n    }\r\n}\r\n.mrg-top{\r\n    margin-top: 2em;\r\n}\r\n\r\n// === ACTIVE STATES ===\r\n:focus{\r\n    outline: none;\r\n}","scss\\partials\\_variables.scss":"$ft-size: 16px;\r\n$base-color: hsl(230, 35%, 7%);\r\n$white: hsl(0, 0%, 100%);\r\n$sec-color: hsl(231, 77%, 90%);","scss\\dest.scss":"@use \"partials/default\" as *;\r\n@use \"partials/mixins\" as *;\r\n@use \"partials/functions\" as *;\r\n@use \"partials/variables\" as *;\r\nbody{\r\n    background-image: url(../assets/destination/background-